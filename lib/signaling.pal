FUNC SIGNAL_LIGHT_TEMP
    SENSE LIGHT
    SENSE TEMP
    MOTOR WALK
    SIGNAL NUCLEUS
    MODIFY PHOS
END FUNC

FUNC SIGNAL_PRESSURE_MAGNETIC
    SENSE PRESSURE
    SENSE MAGNETIC
    SIGNAL MEMBRANE
    MOTOR ROTATE
    MODIFY SUMO
END FUNC

FUNC SIGNAL_ELECTRICAL_COMPLEX
    SENSE ELECTRICAL
    SIGNAL SECRETION
    MOTOR EXPAND
    ATC GFP
    MODIFY UBIQUITIN
END FUNC