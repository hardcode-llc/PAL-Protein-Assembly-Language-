FUNC BIND_DNA_COMPLEX
    BIND DNA
    BIND RNA
    SIGNAL NUCLEUS
    MODIFY PHOS
    MOTOR WALK
END FUNC

FUNC BIND_PROTEIN_COMPLEX
    BIND PROTEIN
    BIND LIPID
    MOTOR ROTATE
    MODIFY SUMO
    SIGNAL SECRETION
END FUNC

FUNC IMMUNE_EVASION_COMPLEX
    IMMUNE EVASION
    MODIFY UBIQUITIN
    SIGNAL CYTOPLASM
    MOTOR CONTRACT
END FUNC